<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.roomapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomApp">
        <activity
            android:label="Wellcome"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.RoomAppBar"
            android:name=".activity.MainActivity"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.EditPengeluaranActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.AddPengeluaranActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>